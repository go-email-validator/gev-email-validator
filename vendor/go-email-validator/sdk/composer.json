{
    "name": "go-email-validator/sdk",
    "description": "SDK for email validator",
    "keywords": [
        "email validator",
        "email validation"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Email Validator",
            "homepage": "https://github.com/go-email-validator"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12",
        "vlucas/phpdotenv": "^5.3"
    },
    "autoload": {
        "psr-4": { "OpenAPI\\Client\\" : "php/lib" }
    },
    "autoload-dev": {
        "psr-4": { "OpenAPI\\Client\\Test\\" : "php/test/" }
    },
    "config": {
        "vendor-dir": "php/vendor"
    }
}
